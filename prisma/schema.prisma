datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id     String  @id @default(auto()) @map("_id") @db.ObjectId
  // slug   String  @unique
  title  String?
  body   String?
  answer String?
}

